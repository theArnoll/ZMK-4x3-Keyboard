/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,physical-layout = &physical_layout0;
        // Other chosen items
    };

    default_layout: default_layout { // https://zmk.dev/docs/development/hardware-integration/physical-layouts#multiple-physical-layouts/
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        transform = <&default_transform>;
        kscan = <&kscan0>;
    };
};
